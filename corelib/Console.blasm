@WriteCharacter($data(Int16)) {
	ncall @WriteFile, %0, $data;					// Redirect to the WriteFile native method, using 0 (stdout) as the first argument
}

@WriteString($data(Array)) {
	ncall @WriteFile, %0, $data;					// Redirect to the WriteFile native method, using 0 (stdout) as the first argument
}
